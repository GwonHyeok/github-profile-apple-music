svg(width=100 height=100)
    defs
        clipPath(id="album-art")
            circle(cx=50 cy=50 r=50 fill=red)
    image(
        width=100
        height=100
        xlink:href=body.data[0].attributes.artwork.url.replace('{w}', '300').replace('{h}', '300')
        clip-path="url(#album-art)"
    )
    div.container(xmlns="http://www.w3.org/1999/xhtml")
        div.text-container
            div.artist #{body.data[0].attributes.artistName}
            div.song #{body.data[0].attributes.name}


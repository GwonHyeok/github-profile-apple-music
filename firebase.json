{
  "functions": {
    "source": "packages/backend",
    "predeploy": [
      "yarn --prefix \"$RESOURCE_DIR\" run lint",
      "yarn --prefix \"$RESOURCE_DIR\" run build"
    ]
  },
  "firestore": {
    "rules": "firestore.rules"
  },
  "emulators": {
    "ui": {
      "enabled": true
    },
    "pubsub": {
      "enabled": true
    },
    "functions": {
      "enabled": true
    },
    "firestore": {
      "enabled": true
    }
  }
}
